{
  "openapi": "3.0.1",
  "info": {
    "title": "Nexus Receiver API",
    "description": "Nexus Receiver API specification. All Nexus supported clients conform to this spec.",
    "contact": {
      "name": "ESD Support",
      "email": "esdsupport@ecco.com"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    },
    "version": "1.0"
  },
  "servers": [
    {
      "url": "/algorithm/v1.2"
    }
  ],
  "paths": {
    "/algorithm/v1.2/complete/{algorithmName}/requests/{requestId}": {
      "post": {
        "tags": [
          "results"
        ],
        "summary": "Completes the provided Nexus algorithm run",
        "description": "Commits the run result to the checkpoint store and transitions the state to COMPLETED",
        "parameters": [
          {
            "name": "algorithmName",
            "in": "path",
            "description": "Request id of the run to complete",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "requestId",
            "in": "path",
            "description": "Request id of the run to complete",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "description": "Run result",
          "content": {
            "*/*": {
              "schema": {
                "$ref": "#/components/schemas/models.AlgorithmResult"
              }
            }
          },
          "required": true
        },
        "responses": {
          "202": {
            "description": "Accepted",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "additionalProperties": {
                    "type": "string"
                  }
                }
              },
              "text/plain": {
                "schema": {
                  "type": "object",
                  "additionalProperties": {
                    "type": "string"
                  }
                }
              },
              "text/html": {
                "schema": {
                  "type": "object",
                  "additionalProperties": {
                    "type": "string"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string"
                }
              },
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              },
              "text/html": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string"
                }
              },
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              },
              "text/html": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "404": {
            "description": "Not Found",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string"
                }
              },
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              },
              "text/html": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        },
        "x-codegen-request-body-name": "payload"
      }
    }
  },
  "components": {
    "schemas": {
      "models.AlgorithmResult": {
        "type": "object",
        "properties": {
          "errorCause": {
            "type": "string",
            "description": "Failure cause, if any."
          },
          "errorDetails": {
            "type": "string",
            "description": "Failure details, if any."
          },
          "resultUri": {
            "type": "string",
            "description": "URL to download results."
          }
        }
      }
    }
  },
  "x-original-swagger-version": "2.0"
}
