          {
            "name": "algorithmName",
            "in": "path",
            "description": "Request id of the run to complete",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "requestId",
            "in": "path",
            "description": "Request id of the run to complete",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
